{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/index.html",
      "check": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "framework": {
    "version": "12.0.5"
  },
  "crons": []
}
